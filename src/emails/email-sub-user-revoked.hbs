---
email_remove_subuser_subject: <%= i18n[language]['general'].email_remove_subuser_subject %>
email_remove_subuser_description: <%= i18n[language]['general'].email_remove_subuser_description %>
title: <%= i18n[language]['general'].email_remove_subuser_subject %>
---
<table class="main" width="100%" cellpadding="0" cellspacing="0" itemprop="action" itemscope itemtype="http://schema.org/ConfirmAction">
  <tr>
    <td class="content-wrap body-content">
      <meta itemprop="name" content="Confirm Email" />
      <table width="100%" cellpadding="0" cellspacing="0">
        <tr>
          <td class="content-block">
            <h2>{{email_remove_subuser_subject}}</h2>
          </td>
        </tr>
        <tr>
          <td class="content-block">
            {{{email_remove_subuser_description}}}
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
