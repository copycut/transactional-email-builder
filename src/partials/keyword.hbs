<td class="keyword-tag">
  <table class="keyword-tag-table {{color}}" cellpadding="0" cellspacing="0">
    <tr>
      <td class="keyword-inside">
        <table class="keyword-inside-table" cellpadding="0" cellspacing="0">
          <tr>
            <td class="keyword-corner">
              <img class="corner" src="{{imgSrc}}/keyword-corner.png" height="28" width="18" alt="" />
            </td>
            <td class="keyword-label">{{label}}</td>
            <td class="keyword-end">
              <img src="{{imgSrc}}/empty.gif" height="28" width="5" alt="" />
            </td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
</td>
