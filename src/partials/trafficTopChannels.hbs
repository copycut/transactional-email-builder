<h4 class="section-small-title">{{weekly_digest_top_channels}}</h4>

\{{#analytics_weekly.data}}
<table class="graph-bar-stacked" width="100%" cellpadding="0" cellspacing="0">
  <tr>
    \{{#each mediums}}
      \{{#eq @index 0}}
        <td class="bar primary"
          style="width:\{{getPercentStacked ../mediums this.current}}%" title="\{{getPercentStacked ../mediums this.current}}%">
          \{{getPercentStacked ../mediums this.current}}%
        </td>
      \{{/eq}}
      \{{#eq @index 1}}
        <td class="bar success"
          style="width:\{{getPercentStacked ../mediums this.current}}%" title="\{{getPercentStacked ../mediums this.current}}%">
          \{{getPercentStacked ../mediums this.current}}%
        </td>
      \{{/eq}}
      \{{#eq @index 2}}
        <td class="bar warning"
          style="width:\{{getPercentStacked ../mediums this.current}}%" title="\{{getPercentStacked ../mediums this.current}}%">
          \{{getPercentStacked ../mediums this.current}}%
        </td>
      \{{/eq}}
    \{{/each}}
  </tr>
</table>

<table class="graph-legend" cellpadding="0" cellspacing="0">
  \{{#each mediums as |medium index|}}
    {{> legendItem
      title="{{key}}"
      previous="{{previous}}"
      current="{{current}}"
      delta="{{delta}}"
      percent="{{abs percent}}%"
      index="{{index}}"
    }}
  \{{/each}}
</table>
\{{/analytics_weekly.data}}
